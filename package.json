{
  "name": "exporeactnativetemplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "clean-android": "cd android && rm -R .gradle && cd app && rm -R build && cd .. && cd ..",
    "build-internal-android": "eas build --platform android --profile preview --clear-cache",
    "build-prod-android": "eas build --platform android --profile production --clear-cache",
    "build-dev-ios": "eas build --platform ios --profile development",
    "build-prod-ios": "eas build --platform ios --profile production --clear-cache",
    "submit-android": "eas submit -p android",
    "submit-ios": "eas submit -p ios",
    "list-env-variables": "eas secret:list",
    "publish-env": "eas secret:push --scope project --env-file .env"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^2.2.6",
    "drizzle-orm": "^0.31.4",
    "expo": "~51.0.38",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.18",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.6",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "i18next": "^23.11.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.2",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "drizzle-kit": "^0.22.8",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
