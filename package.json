{
  "name": "exporeactnativetemplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "clean-android": "cd android && rm -R .gradle && cd app && rm -R build && cd .. && cd ..",
    "build-internal-android": "eas build --platform android --profile preview --clear-cache",
    "build-prod-android": "eas build --platform android --profile production --clear-cache",
    "build-dev-ios": "eas build --platform ios --profile development",
    "build-prod-ios": "eas build --platform ios --profile production --clear-cache",
    "submit-android": "eas submit -p android",
    "submit-ios": "eas submit -p ios",
    "list-env-variables": "eas secret:list",
    "publish-env": "eas secret:push --scope project --env-file .env"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.0.0",
    "@reduxjs/toolkit": "^2.2.6",
    "expo": "^52.0.17",
    "expo-constants": "~17.0.3",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-notifications": "~0.29.11",
    "expo-router": "~4.0.11",
    "expo-splash-screen": "~0.29.16",
    "expo-sqlite": "~15.0.3",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.5",
    "expo-web-browser": "~14.0.1",
    "i18next": "^23.11.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^14.1.2",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
